# LinkHub Dynamic - .env.example

# Core
NODE_ENV=production
PORT=3000
PUBLIC_DOMAIN=example.com
SESSION_SECRET=change-me-please

# Admin seed (first run). If ADMIN_PASSWORD is empty, a strong password is generated and printed once.
ADMIN_USERNAME=admin
ADMIN_PASSWORD=

# Bcrypt
BCRYPT_ROUNDS=12

# Database (MySQL/MariaDB). Works locally or on Namecheap
DB_HOST=localhost
DB_PORT=3306
DB_USER=your_db_user
DB_PASSWORD=your_db_password
DB_NAME=your_db_name
